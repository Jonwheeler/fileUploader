<h1>Contents of bucket</h1>


<% @uploads.each do |upload| %>
    <%= upload.name %>
    <%= link_to 'Download', "#{upload.file}" %>
    <%= upload.created_at.strftime '%d/%m/%y' %>
    <%= link_to 'Edit', edit_upload_path(upload) %>
    <%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></br>
<% end %>

